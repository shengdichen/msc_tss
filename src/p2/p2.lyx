\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\{ \begin{alignedat}{3} & \, & u_{i}^{k} & \coloneqq & \; & \exp\left(-2k^{2}\pi^{2}t\right)\cdot\mathop{\underline{\sin}}\\
 &  & u_{i}^{k}\left(0,\forall x_{1},\forall x_{2}\right) & \coloneqq &  & \mathop{\underline{\sin}}\quad\text{[Temporal Bound.]}\\
 &  & u_{i}^{k}\left(\forall t,\partial_{x_{1}},\partial_{x_{2}}\right) & \equiv &  & 0\qquad\text{[Spatial Bound.]}\\
